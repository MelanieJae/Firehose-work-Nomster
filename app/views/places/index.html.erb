<h1>Welcome to Nomster!</h1>
<h1><% @places.inspect %></h1>


<% @places.each do |place| %>
	<div class="place-box col-xs-10 col-xs-offset-1">
		<h3><%= place.name %></h3><br />
		<i><%= place.address %></i><br /><br />
		<p><%= place.description %></p><br />
	</div>

	<%-#Apple Store pagination style -%> 
<div class="apple_pagination">
		<div class="page_info">
			<%= page_entries_info @places %>
		</div>
	<hr />
<% end %>


